<div class= "container px-5 px-sm-0">
	<h1>ジャンル一覧・追加</h1>
	<%= render 'form', genre: @genre %>
	<table class= "table table-striped">
		<tr>
			<td>ジャンル</td>
			<td>###</td>
			<td></td>
		</tr>
		<% @genres.each do |genre| %>
			<tr>
				<td><%= genre.name %></td>
				<td><%= link_to "編集する", edit_admin_genre_path(genre) %></td>
			</tr>
		<% end %>
	</table>
</div>